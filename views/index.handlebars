<div class="parallax-container">
  <div class="section no-pad-bot">
    <div class="container">
      <br>
      <br>
      <br>
      <br>
      <br>
      <h1 class="header center parallaxText">Tech News Scraper</h1>
    </div>
  </div>
  <div class="parallax"><img src="/assets/img/paralax1.jpg"></div>
</div>

<div class="container">
  <div class="row">
    <div class ="col s12">
      {{#each article}}
        <div class="card-panel center">
          <h4>{{this.title}}</h4>
          <a href = "/articles/{{this._id}}" class="waves-effect waves-light btn articleBtn" id="btnSignUp">Read More</a>
        </div>
      {{/each}}
    </div>
  </div>
</div>